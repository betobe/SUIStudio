{
  "name": "@schibstedspain/sui-studio",
  "version": "2.1.0",
  "description": "SUIComponents studio",
  "main": "index.js",
  "preferGlobal": true,
  "bin": {
    "suistudio": "./bin/suistudio.js"
  },
  "scripts": {
    "phoenix": "rm -Rf node_modules && npm it",
    "co": "git-cz",
    "lint": "standard",
    "up": "updtr --test-stdout --save-exact"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SUI-Components/SUIStudio.git"
  },
  "keywords": [
    "SUIComponents",
    "schibsted",
    "reactjs"
  ],
  "author": "Carlos Villuendas <carlos.villuendas@scmspain.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SUI-Components/SUIStudio/issues"
  },
  "homepage": "https://github.com/SUI-Components/SUIStudio#readme",
  "devDependencies": {
    "commitizen": "2.8.2",
    "cross-env": "2.0.0",
    "cz-conventional-changelog": "1.1.6",
    "ghooks": "1.2.4",
    "npm-run-all": "2.2.2",
    "standard": "7.1.2",
    "updtr": "0.2.0",
    "validate-commit-msg": "2.6.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm-run-all lint",
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "PWD",
      "__BASE_DIR__"
    ]
  },
  "dependencies": {
    "@schibstedspain/suistudio-fatigue-dev": "github:SUI-Components/suistudio-fatigue-dev#9edf485",
    "babel-standalone": "6.12.0",
    "bundle-loader": "0.5.4",
    "codemirror": "5.16.0",
    "colors": "1.1.2",
    "commander": "2.9.0",
    "css-content-loader": "1.0.0",
    "deepmerge": "0.2.10",
    "fs-extra": "0.30.0",
    "fs.realpath": "1.0.0",
    "normalize.css": "4.1.1",
    "pascal-case": "2.0.0",
    "pmx": "0.6.2",
    "react-codemirror": "0.2.6",
    "react-docgen": "2.9.1",
    "react-markdown": "2.3.0",
    "react-router": "2.5.1"
  },
  "suistudio-webpack": {
    "vendor": [
      "react",
      "react-dom"
    ]
  }
}
