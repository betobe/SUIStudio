{
  "name": "sui-studio",
  "version": "0.0.1",
  "description": "SUIComponents studio",
  "main": "index.js",
  "scripts": {
    "co": "git-cz",
    "start": "nodemon studio/server/index.js | bunyan",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SUI-Components/SUIStudio.git"
  },
  "keywords": [
    "SUIComponents",
    "schibsted",
    "reactjs"
  ],
  "author": "Carlos Villuendas <carlos.villuendas@scmspain.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SUI-Components/SUIStudio/issues"
  },
  "homepage": "https://github.com/SUI-Components/SUIStudio#readme",
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "commitizen": "2.8.1",
    "cz-conventional-changelog": "1.1.6",
    "ghooks": "1.2.1",
    "nodemon": "1.9.2",
    "npm-run-all": "1.8.0",
    "standard": "7.0.0",
    "validate-commit-msg": "2.6.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm-run-all lint",
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-3": "6.5.0",
    "babel-standalone": "6.7.7",
    "babelify": "7.3.0",
    "browserify": "13.0.0",
    "browserify-middleware": "7.0.0",
    "bunyan": "1.8.1",
    "bunyan-request": "1.2.0",
    "classnames": "2.2.5",
    "envify": "3.4.0",
    "express": "4.13.4",
    "pmx": "0.6.1",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "simple-html-index": "1.2.0",
    "watchify": "3.7.0",
    "watchify-middleware": "1.6.0"
  }
}
